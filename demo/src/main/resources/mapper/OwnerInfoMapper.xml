<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mountain.mapper.OwnerInfoMapper">

    <!-- 根据用户ID查询车主信息 -->
    <select id="selectByUserId" resultType="com.mountain.entity.OwnerInfo">
        SELECT * FROM owner_info WHERE user_id = #{userId}
    </select>

</mapper>



